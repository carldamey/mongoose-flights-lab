<%- include("../partials/header") %>
<h2>All Flights</h2>
<table>
    <thread>
        <tr>
            <th>Airline</th>
            <th>Airport</th>
            <th>Flight Number</th>
            <th>Departs</th>
        </tr>
    </thread>
    <tbody>
        <% flights.forEach(flight => { %>
            <tr>
                <td><%= flight.airline %></td>
                <td><%= flight.airport %></td>
                <td><%= flight.flightNo %></td>
                <td><%= flight.departs %></td>
            </tr>
        <% }) %>
    </tbody>
</table>
<%- include("../partials/footer") %>